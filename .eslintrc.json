{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jquery": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": { "modules": true }
    },
    "rules": {
        "semi": "off",
        "prettier/prettier": "warn"
    },
    "plugins": ["prettier"]
}
